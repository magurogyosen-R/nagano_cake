<table class="table table-bordered genrebox_table">
  <thead>
    <tr class="table-head">
    <th class="genrebox_head">ジャンル検索</th>
    </tr>
  </thead>
  <tbody>
    <% @genres.each do |genre| %>
    <tr>
      <td class="table-body">
        <span class="btn btn-group-sm btn-success"><%= genre.name %></span>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>